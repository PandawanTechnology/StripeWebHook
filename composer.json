{
    "name": "pandawan-technology/stripe-web-hook",
    "type": "library",
    "description": "Ease the Stripe' API webhook supports",
    "license": "MIT",
    "keywords": ["stripe", "payment", "API", "webhook", "callback", "event"],
    "authors": [
        {
            "name": "Xavier Coureau",
            "email": "xavier@pandawan-technology.com",
            "homepage": "https://www.pandawan-technology.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PandawanTechnology\\StripeWebHook\\": "src/"
